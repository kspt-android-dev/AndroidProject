<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".activities.MainActivity">
    <Button
            android:text="@string/start_game_8"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/start_game_8" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginBottom="60dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.07999998"/>
    <Button
            android:text="@string/start_game_10"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/start_game_10" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/start_game_8"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginBottom="60dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.04000002"/>
    <TextView
            android:text="@string/appname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginBottom="32dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
    <Button
            android:text="@string/resume"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/resume" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/start_game_10" android:layout_marginBottom="60dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.04000002"/>
    <Button
            android:text="@string/button"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/start_fragment"
            app:layout_constraintTop_toBottomOf="@+id/resume"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.04"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
    />

    <Button
            android:text="@string/create_custom"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/create_custom"
            app:layout_constraintTop_toBottomOf="@+id/start_fragment"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.04"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
    />

</androidx.constraintlayout.widget.ConstraintLayout>